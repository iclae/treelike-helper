{
  "name": "treelike-helper",
  "version": "1.0.2",
  "description": "Nested data tool functions",
  "type": "module",
  "files": [
    "dist"
  ],
  "typings": "./dist/treelike-helper.d.ts",
  "main": "./dist/treelike-helper.umd.cjs",
  "module": "./dist/treelike-helper.js",
  "exports": {
    ".": {
      "import": "./dist/treelike-helper.js",
      "require": "./dist/treelike-helper.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "lint": "npx oxlint@latest",
    "prepublishOnly": "npm run test && npm run lint && npm run build"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "immer": "^10.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iclae/treelike-helper.git"
  },
  "homepage": "https://iclae.github.io/treelike-helper-vitepress/",
  "keywords": [
    "tree",
    "nested"
  ],
  "authors": [
    "iclae0@gmail.com"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}